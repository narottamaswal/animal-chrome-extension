{
    "manifest_version": 3,
    "name": "Music Player",
    "description": "A simple music player Chrome extension",
    "version": "1.0.0",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "logo.png"
      }
    },
    "icons": {
      "128": "logo.png"
    }
  }